<%@ Control Language="C#" AutoEventWireup="true" EnableViewState="false" Inherits="BlogEngine.Core.Web.Controls.CommentFormBase" %>
<%@ Import Namespace="BlogEngine.Core" %>
<div class="comment-form well-global">
    <h3 class="well-global-title" id="addcomment"><%=Resources.labels.addComment %></h3>
    <%if (Security.IsAuthenticated)
        { %>    
        <input type="hidden" class="form-control" value="<%=Security.CurrentMembershipUser.UserName %>" name="txtName" id="txtName" readonly />
        <input type="hidden" class="form-control" value="<%=Security.CurrentMembershipUser.Email %>" name="txtEmail" id="txtEmail" readonly />
    <% } else { %>
    <div class="form-group">
        <input type="text" class="form-control" placeholder="<%=Resources.labels.name %>" name="txtName" id="txtName" tabindex="1" required />
    </div>
    <div class="input-group">
        <input type="email" placeholder="<%=Resources.labels.email %>" class="form-control" name="txtEmail" id="txtEmail" tabindex="2" required />
        <a href="//gravatar.com/" target="_blank" class="input-group-addon" id="gravatarmsg">
            <img src="data:image/png;base64," alt="Gravatar Logo" />
            Gravatar enabled&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
        </a>
    </div>
    <% } %>
    <% if (BlogSettings.Instance.EnableWebsiteInComments)
        { %>
    <div class="form-group">
        <input type="text" placeholder="<%=Resources.labels.website%> (optional)" class="form-control" name="txtWebsite" tabindex="3" id="txtWebsite" />
    </div>
    <% } %>
    <div class="form-group" id="commentCompose">
        <textarea class="form-control" id="txtContent" placeholder="<%=Resources.labels.comment%>" cols="50" rows="7" name="txtContent" tabindex="4" required></textarea>
    </div>
    <% if (BlogSettings.Instance.ShowLivePreview)
        { %>
    <div class="form-group comment-preview">
        <label><%=Resources.labels.previewComment %></label>
        <div id="commentPreview">
            <img src="<%=Utils.RelativeWebRoot %>Content/images/blog/ajax-loader.gif" style="display: none" alt="Loading" />
        </div>
    </div>
    <% } %>
    <div class="form-group">
        <label>
            <input type="checkbox" id="cbNotify" />
            <%=Resources.labels.notifyOnNewComments %>
        </label>
    </div>
    <div id="commentCaptcha" class="form-group"></div>
    <input type="button" id="btnSaveAjax" value="<%=Resources.labels.saveComment %>" class="btn btn-primary btn-block" tabindex="5" onclick="return BlogEngine.validateAndSubmitCommentForm()" />
</div>
